{
  "//___README___1": "-------------------------------------------------------------------------------------",
  "//___README___2": "This manifest exists for backwards compatibility purposes only: use demo.json instead",
  "//___README___3": "-------------------------------------------------------------------------------------",
  "devtools_port": 9090,
  "splashScreenImage": "https://web-demo.adaptivecluster.com/static/media/splash-screen.jpg",
  "startup_app": {
    "name": "Reactive Trader Cloud",
    "url": "https://web-demo.adaptivecluster.com/",
    "uuid": "reactive-trader-cloud-web-demo",
    "applicationIcon": "https://web-demo.adaptivecluster.com/static/media/adaptive-mark-large.png",
    "autoShow": true,
    "defaultWidth": 1280,
    "defaultHeight": 900,
    "minWidth": 800,
    "minHeight": 600,
    "resizable": true,
    "maximizable": true,
    "frame": false,
    "_comment": "Openfin Excel API preloaded below + added in appAssets (not included in standard OpenFin package)",
    "preload": [
      {
        "url": "https://web-demo.adaptivecluster.com/plugin/service-loader.js"
      },
      {
        "url": "https://web-demo.adaptivecluster.com/plugin/fin.desktop.Excel.js"
      }
    ]
  },
  "runtime": {
    "arguments": "--force-device-scale-factor=1",
    "version": "10.66.39.43",
    "futureVersion": "10.66.39.43",
    "forceLatest": true
  },
  "shortcut": {
    "company": "Adaptive Consulting",
    "description": "Desktop Reactive Trader Cloud",
    "icon": "https://web-demo.adaptivecluster.com/static/media/icon.ico",
    "name": "Reactive Trader Cloud"
  },
  "appAssets": [
    {
      "src": "https://web-demo.adaptivecluster.com/plugin/add-in.zip",
      "alias": "excel-api-addin",
      "version": "2.0.0",
      "forceDownload": true
    }
  ]
}
