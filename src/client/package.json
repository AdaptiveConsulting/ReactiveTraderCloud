{
  "name": "reactive-trader-cloud",
  "version": "2.1.0",
  "description": "Reactive Trader - Cloud edition. Example reactive currency pair trading app",
  "keywords": [
    "react",
    "fx",
    "spot",
    "cloud",
    "rxjs"
  ],
  "authors": [
    {
      "name": "Adaptive Consulting",
      "email": "@AdaptiveLimited"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdaptiveConsulting/ReactiveTraderCloud.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "start": "npm run dev:cloud",
    "lint": "eslint --ext .js --ext .jsx src",
    "tslint": "./node_modules/.bin/tslint ./**/*.{ts,tsx} -e './node_modules/**/**'",
    "dev:local": "npm run dev -- --env.endpoint local",
    "dev:docker": "npm run dev -- --env.endpoint docker",
    "dev:cloud": "npm run dev -- --env.endpoint demo",
    "dev:openfin:local": "node ./node_modules/npm-run-all/bin/npm-run-all --parallel openfin dev:local",
    "dev:openfin:cloud": "node ./node_modules/npm-run-all/bin/npm-run-all --parallel openfin dev:cloud",
    "dev": "webpack-dev-server --progress --debug --config webpack.config.js",
    "test": "jest --watch",
    "test:dev": "npm test -- --watch",
    "test-inspect": "node inspect ./node_modules/jest/bin/jest.js --runInBand --no-cache",
    "deploy": "webpack --display-error-details",
    "deploy:prod": "set NODE_ENV=production&&webpack",
    "openfin": "openfin -c config/openfin/local.app.json -l",
    "openfin-build-installer": "node ./lib/downloadInstaller.js",
    "stats": "webpack --profile --json > stats.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testMatch": [
      "**/src/**/*.spec.(ts|tsx|js|jsx)"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "verbose": true
  },
  "license": "MIT",
  "dependencies": {
    "animate.css": "3.4.0",
    "autobahn": "17.5.2",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "classnames": "2.2.3",
    "d3": "3.5.12",
    "d3-time-format": "2.0.5",
    "fixed-data-table": "0.6.5",
    "font-awesome": "4.5.0",
    "lodash": "3.10.1",
    "lodash.keyby": "4.6.0",
    "moment": "2.13.0",
    "numeral": "1.5.3",
    "nvd3": "1.8.5",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-nvd3": "0.5.7",
    "react-redux": "5.0.6",
    "react-sizeme": "2.3.6",
    "react-sparklines": "1.7.0",
    "redux": "3.7.2",
    "redux-actions": "^2.2.1",
    "redux-observable": "0.17.0",
    "redux-persist": "5.6.5",
    "redux-persist-crosstab": "3.7.0",
    "reselect": "3.0.1",
    "rxjs": "5.4.2",
    "ua-parser-js": "0.7.10",
    "ws": "0.8.1"
  },
  "devDependencies": {
    "@storybook/react": "3.3.11",
    "@types/autobahn": "^0.9.38",
    "@types/jest": "22.1.1",
    "@types/karma": "1.7.3",
    "@types/lodash": "4.14.100",
    "@types/numeral": "0.0.22",
    "@types/openfin": "17.0.4",
    "@types/react": "16.0.36",
    "@types/react-dom": "16.0.3",
    "@types/react-virtualized": "9.7.10",
    "@types/redux-actions": "2.2.3",
    "@types/storybook__addon-actions": "3.0.1",
    "@types/storybook__addon-links": "3.0.2",
    "@types/storybook__react": "3.0.6",
    "autoprefixer": "7.2.5",
    "awesome-typescript-loader": "3.4.1",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chalk": "^1.1.3",
    "classlist-polyfill": "1.0.3",
    "copy-webpack-plugin": "0.3.3",
    "css-loader": "^0.28.1",
    "dotenv": "1.2.0",
    "enzyme": "^3.3.0",
    "eslint": "4.16.0",
