{
  "name": "new-reactive-trader-cloud",
  "description": "Reactive TraderÂ® - Cloud edition. Example reactive currency pair trading app",
  "sideEffects": false,
  "keywords": [
    "react",
    "fx",
    "spot",
    "cloud",
    "rxjs"
  ],
  "authors": [
    {
      "name": "Adaptive Consulting",
      "email": "@AdaptiveLimited"
    }
