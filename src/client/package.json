{
  "name": "reactive-trader-cloud",
  "description": "Reactive TraderÂ® - Cloud edition. Example reactive currency pair trading app",
  "sideEffects": false,
  "keywords": [
    "react",
    "fx",
    "spot",
    "cloud",
    "rxjs"
  ],
  "authors": [
    {
      "name": "Adaptive Consulting",
      "email": "@AdaptiveLimited"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdaptiveConsulting/ReactiveTraderCloud.git"
  },
  "scripts": {
    "typecheck": "tsc",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --max-warnings 0",
    "lint:cache": "npm run lint -- --cache",
    "_format": "prettier README.md \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "format": "npm run _format -- --write",
    "format:check": "npm run _format -- --check",
    "test": "jest --detectOpenHandles",
    "verify": "npm run typecheck && npm run lint && npm run format:check && npm run test",
    "start": "cross-env TARGET=web vite",
    "build": "cross-env TARGET=web vite build",
    "serve": "cross-env TARGET=web vite preview",
    "openfin:dev": "cross-env TARGET=openfin vite",
    "_openfin:run": "cross-env-shell \"wait-on -l $npm_config_manifest_url && openfin -l -c $npm_config_manifest_url\"",
    "openfin:run:fx": "npm run _openfin:run --manifest_url=http://localhost:1917/config/rt-fx.json",
    "openfin:run:credit": "npm run _openfin:run --manifest_url=http://localhost:1917/config/rt-credit.json",
    "openfin:run:launcher": "npm run _openfin:run --manifest_url=http://localhost:1917/config/launcher.json",
    "openfin:start:fx": "concurrently \"npm:openfin:dev\" \"npm:openfin:run:fx\"",
    "openfin:start:credit": "concurrently \"npm:openfin:dev\" \"npm:openfin:run:credit\"",
    "openfin:start:launcher": "concurrently \"npm:openfin:dev\" \"npm:openfin:run:launcher\"",
    "openfin:build": "cross-env TARGET=openfin vite build",
    "openfin:serve": "cross-env TARGET=openfin vite preview",
    "openfin:start:preview": "npm:openfin:build && concurrently \"npm:openfin:serve\" \"npm:openfin:run:launcher\"",
    "finsemble:dev": "cross-env TARGET=finsemble vite",
    "finsemble:build": "cross-env TARGET=finsemble vite build",
    "generateCod": "hydra-web-codegen -i trading-gateway.hyer -o src/generated",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "e2e:openfin": "playwright test --project=openfin",
    "e2e:web": "playwright test --project=chrome",
    "build-storybook": "build-storybook -o ./dist/storybook"
  },
  "dependencies": {
    "@adaptive/hydra-platform": "^3.120.0",
    "@finos/fdc3": "^1.2.0",
    "@openfin/core": "^27.71.21",
    "@openfin/workspace": "^6.1.6",
    "@openfin/workspace-platform": "^6.1.6",
    "@react-rxjs/core": "^0.10.3",
    "@react-rxjs/utils": "^0.9.5",
    "d3": "^6.6.2",
    "date-fns": "^2.19.0",
    "downshift": "^6.1.9",
    "lodash": "^4.17.21",
    "openfin-adapter": "^27.71.21",
    "openfin-notifications": "^1.10.1",
    "polished": "^4.1.1",
    "query-string": "^7.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "react-measure": "^2.5.2",
    "react-router": "^6.8.2",
    "react-router-dom": "^6.8.2",
    "react-router-hash-link": "^2.4.3",
    "react-switch": "^7.0.0",
    "ress": "^3.0.0",
    "rxjs": "^7.5.4",
    "styled-components": "^5.3.8",
    "typeface-lato": "^1.1.13",
    "typeface-montserrat": "^1.1.13",
    "typeface-roboto": "^1.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@playwright/test": "^1.29.1",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/d3": "^6.3.0",
    "@types/dialogflow": "^0.11.0",
    "@types/google.analytics": "^0.0.42",
    "@types/jest": "^27.0.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^14.14.37",
    "@types/openfin": "^51.0.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-helmet": "^6.1.6",
    "@types/react-measure": "^2.0.8",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-router-hash-link": "^2.4.5",
    "@types/sinon": "^9.0.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitejs/plugin-react": "^1.3.2",
    "concurrently": "^7.6.0",
    "cross-env": "^6.0.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.11",
    "husky": "^4.3.8",
    "jest": "^27.0.0",
    "jest-styled-components": "^7.0.3",
    "openfin-cli": "^3.0.2",
    "playwright": "^1.29.1",
    "prettier": "^2.2.1",
    "react-test-renderer": "^18.2.0",
    "rollup-plugin-modulepreload": "^1.2.3",
    "rollup-plugin-workbox": "^6.2.0",
    "text-encoding": "^0.7.0",
    "ts-jest": "^27.0.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-static-copy": "^0.12.0",
    "wait-on": "^6.0.0"
  }
}
