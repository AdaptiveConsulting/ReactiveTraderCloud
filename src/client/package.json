{
  "name": "reactive-trader-cloud",
  "version": "2.1.0",
  "description": "Reactive Trader - Cloud edition. Example reactive currency pair trading app",
  "keywords": [
    "react",
    "fx",
    "spot",
    "cloud",
    "rxjs"
  ],
  "authors": [
    {
      "name": "Adaptive Consulting",
      "email": "@AdaptiveLimited"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdaptiveConsulting/ReactiveTraderCloud.git"
  },
  "scripts": {
    "start": "npm run start:cloud",
    "start:local": "cross-env REACT_APP_ENV=local npm run start:react-app",
    "start:cloud": "cross-env REACT_APP_ENV=demo npm run start:react-app",
    "start:docker": "cross-env REACT_APP_ENV=docker npm run start:react-app",
    "start:openfin:local": "npm-run-all -p openfin start:local",
    "start:openfin:cloud": "npm-run-all -p openfin start:cloud",
    "start:react-app": "react-app-rewired start --scripts-version react-scripts-ts",
    "build": "npm run build:react-app",
    "build:local": "cross-env REACT_APP_ENV=local npm run build:react-app",
    "build:cloud": "cross-env REACT_APP_ENV=demo npm run build:react-app",
    "build:react-app": "cross-env react-app-rewired build --scripts-version react-scripts-ts",
    "test": "react-app-rewired test --scripts-version react-scripts-ts --env=jsdom",
    "analyze": "source-map-explorer build/static/js/main.*",
    "openfin": "openfin -c public/config/openfin/local.app.json -l",
    "openfin-build-installer": "node ./lib/downloadInstaller.js",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook"
  },
  "license": "MIT",
  "lint-staged": {
    "install/**/*.app.json": [
      "npm run openfin-build-installer",
      "git add ."
    ],
    "src/**/*.{js,ts,tsx,json}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@fortawesome/react-fontawesome": "^0.1.2",
    "ag-grid": "18.1.2",
    "ag-grid-react": "18.1.0",
    "autobahn": "17.5.2",
    "classlist-polyfill": "1.0.3",
    "csstype": "^2.5.7",
    "d3": "3.5.12",
    "d3-time-format": "2.0.5",
    "emotion": "^9.2.6",
    "emotion-theming": "^9.2.6",
    "hoist-non-react-statics": "^2.5.5",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "numeral": "1.5.3",
    "nvd3": "1.8.6",
    "polished": "^1.9.3",
    "react": "^16.5.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.5.1",
    "react-dom-factories": "1.0.2",
    "react-emotion": "^9.2.6",
    "react-helmet": "^5.2.0",
    "react-nvd3": "0.5.7",
    "react-redux": "5.0.7",
    "react-router-dom": "^4.3.1",
    "react-sizeme": "2.3.6",
    "react-spring": "^5.5.4",
    "react-switch": "^3.0.4",
    "redux": "4.0.0",
    "redux-actions": "2.3.2",
    "redux-observable": "1.0.0-alpha.3",
    "reselect": "3.0.1",
    "ress": "^1.2.2",
    "rxjs": "6.1.0",
    "typeface-lato": "^0.0.54",
    "typeface-montserrat": "^0.0.54",
    "ua-parser-js": "0.7.10"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "@babel/polyfill": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@storybook/addon-actions": "^4.0.0-alpha.14",
    "@storybook/addon-centered": "^4.0.0-alpha.14",
    "@storybook/addon-knobs": "^4.0.0-alpha.14",
    "@storybook/addon-links": "^4.0.0-alpha.14",
    "@storybook/addons": "^4.0.0-alpha.14",
    "@storybook/react": "4.0.0-alpha.14",
    "@types/autobahn": "^0.9.38",
    "@types/d3-time-format": "^2.1.0",
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.2.3",
    "@types/karma": "1.7.3",
    "@types/lodash": "4.14.100",
    "@types/numeral": "0.0.22",
    "@types/openfin": "29.0.1",
    "@types/react": "^16.3.11",
    "@types/react-dom": "^16.0.5",
    "@types/react-helmet": "^5.0.7",
    "@types/react-redux": "^6.0.0",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-test-renderer": "^16.0.1",
    "@types/react-virtualized": "9.7.10",
    "@types/redux-actions": "^2.3.0",
    "@types/storybook__addon-actions": "^3.4.0",
    "@types/storybook__addon-knobs": "^3.3.1",
    "@types/storybook__react": "^3.0.8",
    "@types/ua-parser-js": "^0.7.32",
    "awesome-typescript-loader": "^5.2.0",
    "babel-plugin-emotion": "^9.2.6",
    "cross-env": "^5.1.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.1",
    "npm-run-all": "^4.1.2",
    "openfin-cli": "^1.1.6",
    "prettier": "^1.12.1",
    "react-app-rewired": "^1.6.2",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-scripts": "^1.1.5",
    "react-scripts-ts": "^3.1.0",
    "react-test-renderer": "^16.3.2",
    "redux-devtools-extension": "^2.13.2",
    "rxjs-tslint-rules": "^4.1.1",
    "source-map-explorer": "^1.5.0",
    "typescript": "^3.1.1",
    "unzip": "^0.1.11",
    "worker-loader": "^2.0.0"
  }
}
