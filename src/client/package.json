{
  "name": "reactive-trader-cloud",
  "description": "Reactive TraderÂ® - Cloud edition. Example reactive currency pair trading app",
  "keywords": [
    "react",
    "fx",
    "spot",
    "cloud",
    "rxjs"
  ],
  "authors": [
    {
      "name": "Adaptive Consulting",
      "email": "@AdaptiveLimited"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdaptiveConsulting/ReactiveTraderCloud.git"
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-app-rewired start",
    "start:https": "cross-env HTTPS=true react-app-rewired start",
    "start:local-backend": "cross-env REACT_APP_BROKER_HOST=localhost REACT_APP_BROKER_PORT=15674 npm run start",
    "start:dev-backend": "cross-env REACT_APP_BROKER_HOST=web-dev.adaptivecluster.com REACT_APP_BROKER_PORT=80 npm run start",
    "start:demo-backend": "cross-env REACT_APP_BROKER_HOST=web-demo.adaptivecluster.com REACT_APP_BROKER_PORT=80 npm run start",
    "start:uat-backend": "cross-env REACT_APP_BROKER_HOST=web-uat.adaptivecluster.com REACT_APP_BROKER_PORT=80 npm run start",
    "start:openfin": "concurrently npm:start npm:openfin",
    "start:launcher": "concurrently npm:start npm:openfin:launcher",
    "build": "react-app-rewired build",
    "build:finsemble": "cross-env REACT_APP_FINSEMBLE=true react-app-rewired build",
    "build:dev-backend": "cross-env REACT_APP_BROKER_HOST=web-dev.adaptivecluster.com REACT_APP_BROKER_PORT=80 npm run build",
    "build:demo-backend": "cross-env REACT_APP_BROKER_HOST=web-demo.adaptivecluster.com REACT_APP_BROKER_PORT=80 npm run build",
    "test": "react-scripts test --verbose=false",
    "test:ci": "react-scripts test --ci --coverage --reporters=default --reporters=jest-junit",
    "analyze": "source-map-explorer build/static/js/*.chunk.js",
    "openfin": "wait-on -l http://localhost:3000/openfin/app.json && openfin -c http://localhost:3000/openfin/app.json -l",
    "openfin:launcher": "wait-on -l http://localhost:3000/openfin/launcher.json && openfin -c http://localhost:3000/openfin/launcher.json -l",
    "openfin-build-installer": "node ./lib/downloadInstaller.js",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o storybook/build",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx ./src"
  },
  "eslintConfig": {
    "extends": [
      "./.eslintrc.json"
    ]
  },
  "license": "MIT",
