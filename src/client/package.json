  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdaptiveConsulting/ReactiveTraderCloud.git"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint",
    "dev": "cross-env TARGET=web vite",
    "start": "cross-env TARGET=web vite",
    "build": "cross-env TARGET=web vite build",
    "serve": "cross-env TARGET=web vite preview",
    "launcher:dev": "cross-env PORT=1920 TARGET=launcher vite",
    "launcher:build": "cross-env PORT=1920 TARGET=launcher vite build",
    "launcher:run": "wait-on -l http://localhost:1920/dist/config/launcher.json && openfin -c http://localhost:1920/dist/config/launcher.json -l",
    "launcher:start": "concurrently \"npm:launcher:dev\" \"npm:launcher:run\"",
    "openfin:dev": "cross-env TARGET=openfin vite",
    "openfin:build": "cross-env TARGET=openfin vite build",
    "openfin:run": "wait-on -l http://localhost:1917/dist/config/app.json && openfin -c http://localhost:1917/dist/config/app.json -l",
    "openfin:start": "concurrently \"npm:openfin:dev\" \"npm:openfin:run\"",
    "finsemble:dev": "cross-env TARGET=finsemble vite",
    "finsemble:build": "cross-env TARGET=finsemble vite build",
    "format": "prettier --write README.md \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "generateCod": "hydra-web-codegen -i trading-gateway.hyer -o src/generated",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./dist/storybook"
  },
