{
  "name": "reactive-trader-cloud",
  "version": "2.1.0",
  "description": "Reactive Trader - Cloud edition. Example reactive currency pair trading app",
  "keywords": [
    "react",
    "fx",
    "spot",
    "cloud",
    "rxjs"
  ],
  "authors": [
    {
      "name": "Adaptive Consulting",
      "email": "@AdaptiveLimited"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdaptiveConsulting/ReactiveTraderCloud.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "start": "npm run dev:cloud",
    "lint": "eslint --ext .js --ext .jsx src",
    "dev:local": "npm run dev -- --env.endpoint local",
    "dev:docker": "npm run dev -- --env.endpoint docker",
    "dev:cloud": "npm run dev -- --env.endpoint demo",
    "dev:openfin:local": "node ./node_modules/npm-run-all/bin/npm-run-all --parallel openfin dev:local",
    "dev:openfin:cloud": "node ./node_modules/npm-run-all/bin/npm-run-all --parallel openfin dev:cloud",
    "dev": "webpack-dev-server --progress --debug --config webpack.config.js",
    "test": "jest",
    "test:dev": "npm test -- --watch",
    "deploy": "webpack --display-error-details",
    "deploy:prod": "set NODE_ENV=production&&webpack",
    "openfin": "openfin -c config/openfin/local.app.json -l",
    "workbench": "start-storybook -p 9001"
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": {
        "module": "commonjs",
        "jsx": "react"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/src/**/*.spec.(ts|tsx|js)"
    ],
    "verbose": true
  },
  "license": "MIT",
  "dependencies": {
    "animate.css": "3.4.0",
    "autobahn": "0.9.9",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "browser-redux-sync": "1.1.0",
    "classnames": "2.2.3",
    "d3": "3.5.12",
    "d3-time-format": "2.0.5",
    "fixed-data-table": "0.6.3",
    "font-awesome": "4.5.0",
    "lodash": "3.10.1",
    "lodash.keyby": "4.6.0",
    "moment": "2.13.0",
    "numeral": "1.5.3",
    "nvd3": "1.8.5",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-nvd3": "0.5.7",
    "react-redux": "5.0.6",
    "react-sizeme": "2.3.4",
    "react-sparklines": "1.7.0",
    "redux": "3.7.2",
    "redux-actions": "^2.2.1",
    "redux-observable": "0.14.1",
    "redux-persist": "4.8.3",
    "redux-persist-crosstab": "3.5.3",
    "rxjs": "5.4.2",
    "ua-parser-js": "0.7.10",
    "ws": "0.8.1"
  },
  "devDependencies": {
    "@kadira/storybook": "1.33.0",
    "@types/autobahn": "^0.9.35",
    "@types/jest": "20.0.5",
    "@types/karma": "0.13.36",
    "@types/lodash": "3.10.1",
    "@types/react": "16.0.2",
    "@types/react-dom": "15.5.2",
    "@types/react-virtualized": "9.7.3",
    "@types/redux-actions": "1.2.7",
    "autoprefixer": "6.3.1",
    "awesome-typescript-loader": "3.2.2",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-preset-env": "^1.6.0",
    "chalk": "^1.1.3",
    "classlist-polyfill": "1.0.3",
    "copy-webpack-plugin": "0.3.3",
    "css-loader": "^0.28.1",
    "dotenv": "1.2.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-react": "7.1.0",
    "exports-loader": "0.6.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "install": "0.6.1",
    "jasmine-core": "2.4.1",
    "jest": "^20.0.4",
    "json-loader": "0.5.4",
    "node-sass": "^4.5.3",
    "npm-run-all": "2.1.0",
    "openfin-cli": "1.0.2",
    "react-addons-test-utils": "15.6.0",
    "react-test-renderer": "15.6.1",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "react-virtualized": "9.9.0",
    "redbox-react": "1.2.0",
    "redux-devtools-extension": "2.13.2",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "tslint": "^5.5.0",
    "tslint-config-airbnb": "5.2.1",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.0.0",
    "typescript": "2.4.2",
    "url-loader": "^0.5.8",
    "webpack": "^3.4.0",
    "webpack-babel-jest": "^1.0.4",
    "webpack-dev-server": "2.6.1",
    "yargs": "3.31.0"
  }
}
