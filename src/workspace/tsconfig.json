{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"]
    },
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "Node",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "types": ["vite/client", "./fin"]
  },
  "include": ["./src"]
}
