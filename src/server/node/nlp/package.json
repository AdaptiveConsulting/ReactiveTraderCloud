  "devDependencies": {
    "@types/dialogflow": "^0.11.0",
    "@types/dotenv": "^6.1.0",
    "@types/node": "^11.9.3",
    "@types/uuid": "^3.4.4",
    "copy-webpack-plugin": "^5.1.0",
    "nodemon": "^1.19.3",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  }
